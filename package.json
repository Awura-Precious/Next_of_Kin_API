{
	"name": "starter-express",
	"version": "1.1.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node build/src/server.js",
		"build": "npm run cleanup && npm run format && tsc --pretty",
		"dev": "ts-node-dev --respawn --transpile-only src/server.ts",
		"cleanup": "rm -fr build && rm -fr logs",
		"db:pull": "prisma db pull && prisma generate",
		"db:push": "prisma db push && prisma generate",
		"format": "npm run lint && npm run prettier",
		"lint": "npm run lint:check && npm run lint:fix",
		"lint-staged": "lint-staged",
		"lint:check": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts",
		"prepare": "husky install",
		"prettier": "npm run prettier:check && npm run prettier:fix",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"pretty-quick": "pretty-quick --staged",
		"release": "standard-version",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watchAll"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^3.11.1",
		"axios": "^0.26.1",
		"config": "^3.3.7",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"helmet": "^5.0.2",
		"hpp": "^0.2.3",
		"moment": "^2.29.2",
		"morgan": "^1.10.0",
		"morgan-json": "^1.1.0",
		"uuid": "^8.3.2",
		"winston": "^3.6.0",
		"winston-daily-rotate-file": "^4.7.1",
		"xml2js": "^0.4.23",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@types/config": "^0.0.41",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/hpp": "^0.2.2",
		"@types/jest": "^27.4.1",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/md5": "^2.3.2",
		"@types/moment": "^2.13.0",
		"@types/morgan": "^1.9.3",
		"@types/morgan-json": "^1.1.0",
		"@types/node": "^17.0.23",
		"@types/uuid": "^8.3.4",
		"@types/xml2js": "^0.4.11",
		"@types/yup": "^0.29.13",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.12.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^16.2.0",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"lint-staged": "^12.3.7",
		"prettier": "^2.6.2",
		"pretty-quick": "^3.1.3",
		"prisma": "^3.11.1",
		"standard-version": "^9.3.2",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.6.3"
	},
	"lint-staged": {
		"*.ts": "eslint --fix"
	}
}
